{
  "name": "profire-flamegraph",
  "version": "0.0.2",
  "description": "Profire flamegraph visualization library based on pyroscope-io's flamegraph",
  "author": "Tianlin Yang",
  "repository": {
    "type": "git",
    "url": "https://github.com/TLin-Y/pyroscope.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.node.js",
  "browser": "dist/index.js",
  "_types": "since we are importing stuff from webapp (ui components), tsc ends up generating a weird file tree",
  "types": "dist/packages/profire-flamegraph/src/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "src/**/*",
    "dist/**/*",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "watch": "npm build:lib --watch",
    "dev": "chokidar --initial \"src/**/*\" -c \"npm run build:lib\"",
    "build": "npm run build:types && npm run build:lib",
    "build:lib": "set NODE_ENV=production&& webpack --config ../../scripts/webpack/webpack.flamegraph.ts",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly && downlevel-dts dist dist",
    "build:types:watch": "tsc -p tsconfig.json --emitDeclarationOnly --watch --preserveWatchOutput",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint ./ --cache --fix"
  },
  "peerDependencies": {
    "graphviz-react": "^1.2.5",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0",
    "true-myth": "^5.1.2"
  },
  "devDependencies": {
    "@pyroscope/models": "^0.4.7"
  }
}
