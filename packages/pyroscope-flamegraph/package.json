{
  "name": "@pyroscope/flamegraph",
  "version": "0.2.0",
  "main": "dist/index.node.js",
  "browser": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist/**/*",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "jest",
    "build": "yarn build:types && NODE_ENV=production webpack --config ../../scripts/webpack/webpack.flamegraph.ts",
    "build:types": "tsc -p tsconfig.json --noEmitJs",
    "build:types:watch": "tsc -p tsconfig.json --noEmitJs",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "dev": "NODE_ENV=production webpack --config ../../scripts/webpack/webpack.flamegraph.ts --watch",
    "watch": "npm-run-all dev build:types:watch"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "devDependencies": {
    "@pyroscope/models": "^0.0.3"
  },
  "dependencies": {
    "true-myth": "^5.1.2"
  }
}
